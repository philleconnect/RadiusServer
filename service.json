{
    "containers":[
        {
            "name":"freeradius",
            "hostname":"freeradius",
            "url":"https://github.com/philleconnect/RadiusServer/releases/download/v2.0.2/freeradius.tar.gz",
            "ports":[
                {
                    "external":"1812/udp",
                    "internal":1812
                },
                {
                    "external":1813,
                    "internal":1813
                },
                {
                    "external":18120,
                    "internal":18120
                }
            ],
            "networks":[
                {
                    "name":"schoolconnect"
                },
                {
                    "name":"ldap-net"
                }
            ],
            "volumes":[],
            "environment":[
                "RADIUS_LISTEN_IP_RANGE",
                "RADIUS_PASSWORD",
                "RADIUS_REQUIRE_TEACHER",
                "SLAPD_PASSWORD",
                "SLAPD_DOMAIN0",
                "SLAPD_DOMAIN1"
            ]
        }
    ],
    "volumes":[],
    "networks":[],
    "environment":[
        {
            "name":"RADIUS_LISTEN_IP_RANGE",
            "description":"The IP-Range or specific IP the Radius-Server shall listen to.  Make sure all IPs of all access points are covered. Examples are '192.168.178.0/24' for a 192.168.178.*-network or '172.16.0.0/16' for a 172.16.*.*-network.",
            "mutable":true
        },
        {
            "name":"RADIUS_PASSWORD",
            "description":"Passwords for the Radius-Server that will need to be entered in the access points.",
            "mutable":true
        },
        {
            "name":"SLAPD_PASSWORD",
            "description":""
        },
        {
            "name":"SLAPD_DOMAIN0",
            "description":""
        },
        {
            "name":"SLAPD_DOMAIN1",
            "description":""
        }
    ]
}
